    <file source-language="fr">
        <header/>
        <body>

            <!--###########################################-->
            <!--###  BACKEND MODULE - DEFINITION ERROR  ###-->
            <!--###########################################-->

            <trans-unit id="info_box.title">
                <target>Erreur de définition de NotiZ</target>
            </trans-unit>

            <trans-unit id="info_box.error.header">
                <target>Au moins une erreur de définition a été trouvée.</target>
            </trans-unit>
            <trans-unit id="info_box.error.body">
                <target>Vous devez régler ces erreurs pour que NotiZ puisse fonctionner correctement.</target>
            </trans-unit>

            <trans-unit id="info_box.editor.title">
                <target>Erreur de configuration</target>
            </trans-unit>
            <trans-unit id="info_box.editor.error.body">
                <target>Des erreurs ont été trouvées dans la configuration du module, veuillez contacter votre administrateur.</target>
            </trans-unit>

            <trans-unit id="info_box.exception.header">
                <target>Une erreur PHP est survenue pendant le processus de création de la définition.</target>
            </trans-unit>
            <trans-unit id="info_box.exception.body">
                <target>Vous devez régler ces erreurs pour que NotiZ puisse fonctionner correctement. Avez-vous tenté de :</target>
            </trans-unit>
            <trans-unit id="info_box.exception.body.help_1">
                <target>Vider les caches TYPO3 ?</target>
            </trans-unit>
            <trans-unit id="info_box.exception.body.help_2">
                <target>Vider les caches depuis l'outil d'installation (« install tool ») ?</target>
            </trans-unit>
            <trans-unit id="info_box.exception.body.help_3">
                <target>Analyser l'erreur ci-dessous et essayer de régler le problème ?</target>
            </trans-unit>
            <trans-unit id="info_box.exception.body.help_4">
                <target><![CDATA[Demander de l'aide dans <a href="https://github.com/CuyZ/NotiZ/issues" target="_blank">https://github.com/CuyZ/NotiZ/issues</a>? Fournissez un maximum d'informations.]]></target>
            </trans-unit>
            <trans-unit id="info_box.exception.body.help_5">
                <target>Prier le dieu de TYPO3 ?</target>
            </trans-unit>

            <trans-unit id="info_box.exception.details">
                <target>Détails de l'exception</target>
            </trans-unit>
            <trans-unit id="info_box.exception.details.code">
                <target>Code de l'exception</target>
            </trans-unit>
            <trans-unit id="info_box.exception.details.message">
                <target>Message de l'exception</target>
            </trans-unit>

            <trans-unit id="info_box.exception.details_advanced">
                <target>Détails avancés</target>
            </trans-unit>
            <trans-unit id="info_box.exception.open_in_new_tab">
                <target>Ouvrir dans un nouvel onglet</target>
            </trans-unit>

            <trans-unit id="errors_link.label">
                <target>Montrer %s erreur(s)</target>
            </trans-unit>
            <trans-unit id="errors_count">
                <target>%s erreur(s)</target>
            </trans-unit>
        </body>
    </file>
