            <!--###    EMAIL NOTIFICATION RECORD   ###-->
            <!--######################################-->
            <trans-unit id="title">
                <target>E-Mail</target>
            </trans-unit>
            <trans-unit id="description">
                <target><![CDATA[
                    DE : Emails will be sent to configured recipients.

                    Custom layouts can be used to change the rendering of the mail body.

                    Markers can be used within the title and the body to be replaced with dynamic values when the event is dispatched.
                ]]></target>
            </trans-unit>

            <!-- DEFINITION -->
