                <target>E-Mail-Konfiguration</target>
            </trans-unit>
            <trans-unit id="tab.mail_recipients">
                <target>E-Mail-Empfänger</target>
            </trans-unit>
            <trans-unit id="tab.mail_sender">
                <target>E-Mail-Absender</target>
            </trans-unit>

            <!-- PALETTES -->
            <trans-unit id="palette.mail">
                <target>E-Mail</target>
            </trans-unit>
            <trans-unit id="palette.mail_recipients_to">
