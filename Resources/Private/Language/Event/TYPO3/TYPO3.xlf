            <trans-unit id="cache_cleared.title">
                <source>Caches cleared</source>
            </trans-unit>
            <trans-unit id="cache_cleared.description">
                <source>This event will be triggered whenever caches are manually cleared within the TYPO3 instance.</source>
            </trans-unit>

            <trans-unit id="cache_cleared.flex_form.allowed_commands.label">
                <source>Allowed commands</source>
            </trans-unit>

            <trans-unit id="cache_cleared.marker.cache_command">
                <source>Name of the executed cache command (common values are `system`, `page`, â€¦).</source>
            </trans-unit>
            <trans-unit id="cache_cleared.marker.page_uid">
                <source>If the executed cache command is `page`, this marker contains the uid of the page of which cache was cleared.</source>
            </trans-unit>

            <!--#############################-->
            <!--###  EXTENSION INSTALLED  ###-->
            <!--#############################-->

            <trans-unit id="extension_installed.title">
                <source>Extension installed</source>
            </trans-unit>
            <trans-unit id="extension_installed.description">
                <source>This event will be triggered when an extension is installed on the TYPO3 instance.</source>
            </trans-unit>

            <trans-unit id="extension_installed.marker.key">
                <source>Key of the extension, for instance `my_extension`.</source>
            </trans-unit>
            <trans-unit id="extension_installed.marker.title">
                <source>Title of the extension, for instance `My extension`.</source>
            </trans-unit>
            <trans-unit id="extension_installed.marker.description">
                <source>Description of the extension.</source>
            </trans-unit>
            <trans-unit id="extension_installed.marker.version">
                <source>Version of the extension, for instance `1.4.2`.</source>
            </trans-unit>

            <!--################################-->
            <!--###  BACKEND USER LOGGED IN  ###-->
            <!--################################-->

            <trans-unit id="backend_user_logged_in.title">
                <source>Backend user logged in</source>
            </trans-unit>
            <trans-unit id="backend_user_logged_in.description">
                <source>This event will be triggered when a backend user just logged in (with success).</source>
            </trans-unit>

            <trans-unit id="backend_user_logged_in.marker.user">
                <source>Information about the user that just logged in.</source>
            </trans-unit>
            <trans-unit id="backend_user_logged_in.marker.email">
                <source>User that just logged in</source>
            </trans-unit>

